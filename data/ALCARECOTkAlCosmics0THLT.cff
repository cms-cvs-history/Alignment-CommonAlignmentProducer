//_________________________________HLT bits___________________________________________
 
  module  ALCARECOTkAlCosmicsCTF0THLT = hltHighLevel from "HLTrigger/HLTfilters/data/hltHighLevel.cfi"
  replace ALCARECOTkAlCosmicsCTF0THLT.andOr = true # choose logical OR between Triggerbits
  replace ALCARECOTkAlCosmicsCTF0THLT.HLTPaths = {"CandHLTTrackerCosmicsCTF"}

  module  ALCARECOTkAlCosmicsCosmicTF0THLT = hltHighLevel from "HLTrigger/HLTfilters/data/hltHighLevel.cfi"
  replace ALCARECOTkAlCosmicsCosmicTF0THLT.andOr = true # choose logical OR between Triggerbits
  replace ALCARECOTkAlCosmicsCosmicTF0THLT.HLTPaths = {"CandHLTTrackerCosmicsCoTF"}

  module  ALCARECOTkAlCosmicsRS0THLT = hltHighLevel from "HLTrigger/HLTfilters/data/hltHighLevel.cfi"
  replace ALCARECOTkAlCosmicsRS0THLT.andOr = true # choose logical OR between Triggerbits
  replace ALCARECOTkAlCosmicsRS0THLT.HLTPaths = {"CandHLTTrackerCosmicsRS"}

//________________________________Track selection____________________________________
// take from stream that is HLT-less

  include "Alignment/CommonAlignmentProducer/data/ALCARECOTkAlCosmics0T.cff"

//________________________________Sequences____________________________________
// simply add HLT in front of HLT-less paths

  sequence seqALCARECOTkAlCosmicsCTF0THLT      = { ALCARECOTkAlCosmicsCTF0THLT & seqALCARECOTkAlCosmicsCTF0T }
  sequence seqALCARECOTkAlCosmicsCosmicTF0THLT = { ALCARECOTkAlCosmicsCosmicTF0THLT & seqALCARECOTkAlCosmicsCosmicTF0T }
  sequence seqALCARECOTkAlCosmicsRS0THLT       = { ALCARECOTkAlCosmicsRS0THLT & seqALCARECOTkAlCosmicsRS0T }
